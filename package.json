{
  "name": "mt-weui-react",
  "version": "0.1.1",
  "description": "weui for react",
  "main": "./lib",
  "scripts": {
    "start": "webpack-dev-server --hot --inline --progress --colors --port 8081",
    "build:less": "find lib/ -name '*.less' -exec lessc -x {} \\; > ./lib/react-weui.min.css",
    "build": "yarn run clean & NODE_ENV=production babel ./src --out-dir ./lib --copy-files && yarn run build:less",
    "build:example": "webpack --config webpack.config.prod.js --progress --colors -p",
    "lint": "eslint 'src/**/*.js'",
    "test": "mocha --compilers js:babel-core/register --recursive -r ignore-styles -r jsdom-global/register",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/git-lt/weui-react"
  },
  "keywords": [
    "mobile",
    "ui",
    "framework",
    "weui",
    "react"
  ],
  "author": "aitter",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/git-lt/weui-react/issues"
  },
  "homepage": "http://aitter.oschina.io/",
  "dependencies": {
    "babel-runtime": "^6.18.0",
    "classnames": "^2.2.0",
    "mobile-detect": "^1.3.3",
    "pubsub-js": "^1.5.0",
    "react": "^0.13.3",
    "semver": "^5.3.0",
    "sort-object": "^3.0.2",
    "warning": "^3.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.5",
    "babel": "^6.0.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.0.0",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-transform-require-ignore": "0.0.2",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-standalone": "^6.18.2",
    "codemirror": "^5.20.2",
    "css-loader": "^0.23.0",
    "docute-cli": "^0.6.0",
    "enzyme": "^2.6.0",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.11.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "fastclick": "^1.0.6",
    "file-loader": "^0.8.5",
    "glob": "^7.1.2",
    "history": "^1.17.0",
    "html-webpack-plugin": "^2.14.0",
    "ignore-styles": "^5.0.1",
    "js-yaml": "^3.9.0",
    "less": "^2.5.3",
    "less-loader": "^2.2.1",
    "mkdirp": "^0.5.1",
    "mocha": "^2.3.4",
    "nyc": "^9.0.1",
    "open-browser-webpack-plugin": "0.0.1",
    "postcss": "^5.2.10",
    "postcss-loader": "^1.2.2",
    "raw-loader": "^0.5.1",
    "react-router": "^0.13.3",
    "rimraf": "^2.6.1",
